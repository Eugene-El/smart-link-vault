{
    "manifest_version": 3,
    "name": "Smart Link Vault",
    "version": "1.6.0",
    "description": "Angular Chrome Extension for usefull links and sessions save",
    "icons": {
        "16": "assets/icons/vault-16.png",
        "46": "assets/icons/vault-46.png",
        "128": "assets/icons/vault-128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "tabGroups",
        "storage"
    ]
}